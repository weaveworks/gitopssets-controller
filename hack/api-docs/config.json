{
  "hideMemberFields": ["TypeMeta"],
  "hideTypePatterns": ["ParseError$", "List$"],
  "externalPackages": [
    {
      "typeMatchPrefix": "^k8s\\.io/(api|apimachinery|apiextensions-apiserver/pkg/apis|runtime)/",
      "docsURLTemplate": "https://kubernetes.io/docs/reference/generated/kubernetes-api/v1.19/#{{lower .TypeIdentifier}}-{{arrIndex .PackageSegments -1}}-{{arrIndex .PackageSegments -2}}"
    },
    {
      "typeMatchPrefix": "^github.com/fluxcd/pkg/apis/meta",
      "docsURLTemplate": "https://godoc.org/github.com/fluxcd/pkg/apis/meta#{{ .TypeIdentifier }}"
    },
    {
      "typeMatchPrefix": "^github.com/gitopssets-controller/api/v1alpha1",
      "docsURLTemplate": "https://godoc.org/github.com/gitopssets-controller/api/v1alpha1#{{ .TypeIdentifier }}"
    }
  ],
  "typeDisplayNamePrefixOverrides": {
    "k8s.io/api/": "Kubernetes ",
    "k8s.io/apiextensions-apiserver/": "Kubernetes ",
    "k8s.io/apimachinery/pkg/apis/": "Kubernetes "
  },
  "markdownDisabled": false
}
